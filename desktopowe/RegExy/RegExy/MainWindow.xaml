<Window x:Class="RegExy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegExy"
        mc:Ignorable="d"
        Title="RegEx" Height="450" Width="450">
    <Grid>
        <Label Content="Kod pocztowy" HorizontalAlignment="Left" Margin="114,110,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtBoxKodpocztowy" HorizontalAlignment="Left" Margin="114,141,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnKodpocztowy" Content="Sprawdź kod" HorizontalAlignment="Left" Margin="260,140,0,0" VerticalAlignment="Top" Click="btnKodpocztowy_Click_1"/>
        <TextBox x:Name="txtBoxEmail" HorizontalAlignment="Left" Margin="114,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Center" Width="120"/>
        <Label Content="E-mail" HorizontalAlignment="Left" Margin="114,177,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnEmail" Content="Sprawdź email" HorizontalAlignment="Left" Height="18" Margin="260,0,0,0" VerticalAlignment="Center" Width="89" Click="btnEmail_Click"/>

    </Grid>
</Window>
